
@{
    ViewBag.Title = "Archive";
    Layout = "~/Areas/adminstrator/Views/Shared/_adminLayout.cshtml";
}

<center><h2>Les Mandats Archivés</h2></center>


<form class="sidebar-search">
    <div class="input-box">
        <button type="submit" class="submit">
            <i class="icon-search"></i>
        </button>
        <span>
            <input type="text" placeholder="Search...">
        </span>
    </div>
</form>

<div class="row">
    <div class="col-md-12">
        <div class="widget box">

            <div class="widget-content">
                <table class="table table-striped table-bordered table-hover table-checkable datatable">
                    <thead>
                        <tr>

                            <th>id Mandat</th>
                            <th>Date debut</th>
                            <th>Date fin</th>
                            <th>Ressource</th>
                            <th>Projet</th>
                          


                        </tr>
                    </thead>
                    <tbody>



                        @foreach (var p in ViewBag.res)
                        {
                            <tr>

                                <td>@p.idMandat</td>
                                <td>@p.dateDebut.ToShortDateString().ToString()</td>
                                <td>@p.dateFin.ToShortDateString().ToString()</td>
                                <td>@p.ressource.nom @p.ressource.prenom</td>
                                <td>@p.projet.NomProjet</td>


                            </tr>
                        }




                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>



